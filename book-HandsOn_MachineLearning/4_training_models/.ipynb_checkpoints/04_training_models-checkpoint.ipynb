      "[[-0.98107765]\n",
      " [-0.77323872]]\n",
      "\n",
      "Theta final: \n",
      "[[4.1654969]\n",
      " [2.8524202]]\n"
     ]
    }
   ],
   "source": [
    "# Testando na prática\n",
    "lamda = 0.1 # learning rate\n",
    "n_iterations = 1000\n",
    "m = X.shape[0]\n",
    "\n",
    "# Inicialização randomica\n",
    "theta = np.random.randn(2, 1)\n",
    "print(f'Theta inicial: \\n{theta}')\n",
    "\n",
    "# Laço de iteração\n",
    "for i in range(n_iterations):\n",
    "    gradients = X_train.T.dot(X_train.dot(theta) - y) / (2 * m)\n",
    "    theta = theta - lamda * gradients\n",
    "print(f'\\nTheta final: \\n{theta}')"
   ]
